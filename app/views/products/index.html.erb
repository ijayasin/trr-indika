<h1>Products</h1>


<ul data-turbolinks="true">
<% product_aggregations_filters(@products) do |name, selected, facet_url| %>
  <li><%= link_to(name, facet_url) %> (<%= selected %>)</li>
<% end %>
</ul>


<% #= render :partial => 'products_table', :locals => {:products => @products} %>


<div class="container">
  <div class="row">
    <%= render :partial => 'product', :collection => @products %>
  </div>
</div>


<%= javascript_tag do %>
var Turbolinks = require("turbolinks");
Turbolinks.start();
<% end %>



